{
  "author": "maxbirkner",
  "dependencies": {
    "@actions/artifact": "^2.1.13",
    "@actions/core": "^1.11.1",
    "@actions/github": "^6.0.1",
    "@octokit/auth-app": "^8.0.2",
    "d3": "^7.8.5",
    "d3-hierarchy": "^3.1.2",
    "jsdom": "^23.0.1",
    "picomatch": "^4.0.3",
    "sharp": "^0.33.4"
  },
  "description": "GitHub Action that analyzes code coverage for files changed in a pull request and generates a visual treemap",
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/d3": "^7.4.3",
    "@types/jest": "^30.0.0",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^24.3.0",
    "@types/picomatch": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "@vercel/ncc": "^0.38.2",
    "eslint": "^9.33.0",
    "jest": "^30.0.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.1"
  },
  "keywords": [
    "github-action",
    "coverage",
    "lcov",
    "treemap",
    "testing"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "name": "coveragemap-action",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxbirkner/coveragemap.git"
  },
  "scripts": {
    "build": "tsc",
    "format": "prettier --write src/**/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build src/index.ts -o dist --source-map --license licenses.txt",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "version": "1.0.0"
}
